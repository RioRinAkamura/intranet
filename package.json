    "lodash": "4.17.21",
    "markdown-draft-js": "2.2.1",
    "node-plop": "0.26.2",
    "papaparse": "5.3.0",
    "plop": "2.7.4",
    "prettier": "2.1.1",
    "prettier-eslint-cli": "5.0.1",
    "query-string": "7.0.0",
    "react": "16.14.0",
    "react-app-polyfill": "1.0.6",
    "react-avatar": "3.10.0",
    "react-beautiful-dnd": "13.1.0",
